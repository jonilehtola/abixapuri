{
	"manifest_version": 2,
	"name": "AbixApuri",
	"version": "0.1.2",
	"description": "AbixApuri tuo lisätoiminnallisuutta oma.abitti.fi-palveluun",
	"icons" : {
		"96": "icons/abix-96.png"
	},
	"web_accessible_resources": [
		"assets/*", "assets/ckeditor/*"
	],
	"content_scripts": [
		{
			"matches": ["*://oma.abitti.fi/*"],
			"js": ["settings.js","assets/jquery-3.2.1.min.js", "assets/Sortable.js", "assets/ckeditor/ckeditor.js", "abixapuri.js"]
		}
	],
	"permissions": [
		"*://oma.abitti.fi/*"
	]
}